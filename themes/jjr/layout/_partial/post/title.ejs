<% if (post.link){ %>
  <h1 itemprop="name">
    <a class="<%= class_name %>" href="<%- post.link %>" target="_blank" itemprop="url"><%- post.typTitle %></a>
  </h1>
<% } else if (post.typTitle){ %>
  <% if (index){ %>
    <h1 itemprop="name">
      <a class="<%= class_name %>" href="<%- config.root %><%- post.path %>"><%- post.typTitle %></a>
    </h1>
  <% } else { %>
    <h1 class="<%= class_name %>" itemprop="name">
      <%- post.typTitle %>
    </h1>
  <% } %>
<% } %>