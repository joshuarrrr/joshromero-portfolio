<header class="site-header">
  <div class="nav-title">
    <div class="name"><a href="<%- config.root %>">Josh Romero</a></div>
    <div class="description"><%- config.subtitle %></div>
  </div>
  <nav role="navigation">
    <ul class="main-nav">
      <% for (var i in theme.menu){ %>
        <li class="nav-item">
          <a class="main-nav-link no-underline
          <% if (navitem === theme.menu[i]) { %> active<% } %>
          " href="<%- theme.menu[i] %>"><%= i %></a>
        </li>
      <% } %>
    </ul>
  </nav>
</header>
